#include <iostream>
int main()
{
    double liczba = 123.3;
    std::cout << "liczba = " << liczba << std::endl;
   
    liczba = liczba + 6.7;
    std::cout << "liczba = " << liczba << std::endl;
   
    liczba -= 30;
    std::cout << "liczba = " << liczba << std::endl;
   
    liczba /= 11;
    std::cout << "liczba = " << liczba << std::endl;
   
    liczba *= 3;
    std::cout << "liczba = " << liczba << std::endl;
   
    int nowa = liczba;
    std::cout << "nowa = " << nowa << std::endl;
   
    nowa %= 8;
    std::cout << "nowa = " << nowa << std::endl;
   
    nowa++;
    std::cout << "nowa = " << nowa << std::endl;
   
    ++nowa;
    std::cout << "nowa = " << nowa << std::endl;
   
    nowa *= 10;
    std::cout << "nowa = " << nowa << std::endl;
   
    --nowa;
    std::cout << "nowa = " << nowa << std::endl;
   
    nowa--;
    std::cout << "nowa = " << nowa << std::endl;
    return 0;
}
